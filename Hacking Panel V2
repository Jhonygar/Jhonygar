.bx-burst, .bx-burst-hover:hover { animation: 1.5s linear 0s infinite normal none running burst; }

.bx-fade-up, .bx-fade-up-hover:hover { animation: 1.5s linear 0s infinite normal none running fade-up; }

.bx-fade-down, .bx-fade-down-hover:hover { animation: 1.5s linear 0s infinite normal none running fade-down; }

.bx-fade-left, .bx-fade-left-hover:hover { animation: 1.5s linear 0s infinite normal none running fade-left; }

.bx-fade-right, .bx-fade-right-hover:hover { animation: 1.5s linear 0s infinite normal none running fade-right; }

